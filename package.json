{
  "name": "conuhacks2019",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Typescript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon server/server.ts",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": "server/**/*",
    "ext": "ts, hbs, scss",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afiorito/conuhacks2019.git"
  },
  "author": "",
