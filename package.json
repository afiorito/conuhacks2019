  "bugs": {
    "url": "https://github.com/afiorito/conuhacks2019/issues"
  },
  "homepage": "https://github.com/afiorito/conuhacks2019#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  }
}
